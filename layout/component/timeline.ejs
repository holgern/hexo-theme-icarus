<div class="card widget">
  <div class="card-content">
    <% if (typeof(head) !== 'undefined' && head) { %>
      <h3 class="tag is-link">
        <%= head %>
      </h3>
    <% } %>
    <div class="timeline">
      <% posts.each(post => { %>
        <%- partial("component/cached_item", {post}) %>
      <% }) %>
    </div>
  </div>
</div>
